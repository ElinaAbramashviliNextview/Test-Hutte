<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>vacRep__Only_one_object</fullName>
    <active>true</active>
    <description>An object or a special object must be selected</description>
    <errorConditionFormula>NOT(ISBLANK(vacRep__Object__c)) &amp;&amp; NOT(TEXT(vacRep__Special_Objects__c) == &apos;&apos;)</errorConditionFormula>
    <errorMessage>Only one object can be selected</errorMessage>
</ValidationRule>
