<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>vacRep__Object_must_be_selected</fullName>
    <active>true</active>
    <description>An object or a special object must be selected</description>
    <errorConditionFormula>ISBLANK(vacRep__Object__c) &amp;&amp; TEXT(vacRep__Special_Objects__c) == &apos;&apos;</errorConditionFormula>
    <errorMessage>One object must be selected</errorMessage>
</ValidationRule>
