<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IsEditableCheckedCheckIsVisible</fullName>
    <active>true</active>
    <errorConditionFormula>AND( IsEditable__c == true,  IsVisible__c == false)</errorConditionFormula>
    <errorDisplayField>IsVisible__c</errorDisplayField>
    <errorMessage>Checkbox Editable was checked so you must check field Visible.</errorMessage>
</ValidationRule>
