<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RealizationStartDate_Must_Be_Filled</fullName>
    <active>true</active>
    <description>Realization End date as well</description>
    <errorConditionFormula>AND(
OR(ISBLANK(RealizationStartDate__c),ISBLANK(RealizationEndDate__c)),
ISPICKVAL(StageName, &quot;Closed&quot;)
)</errorConditionFormula>
    <errorMessage>Realization Start and End dates must be filled before you can move our record to the next stage.</errorMessage>
</ValidationRule>
