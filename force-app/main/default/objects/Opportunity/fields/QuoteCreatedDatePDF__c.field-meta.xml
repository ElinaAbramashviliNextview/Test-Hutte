<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>QuoteCreatedDatePDF__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
    ISBLANK(SyncedQuoteId),
    &apos;&apos;,
    TEXT(DAY(DATEVALUE(SyncedQuote.CreatedDate)))+&quot;.&quot;+
    TEXT(MONTH(DATEVALUE(SyncedQuote.CreatedDate)))+&quot;.&quot;+
    TEXT(YEAR(DATEVALUE(SyncedQuote.CreatedDate)))
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Quote Created Date (PDF)</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
